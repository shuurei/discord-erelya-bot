enum MemberVaultCapacityTier {
  TIER_0
  TIER_1
  TIER_2
  TIER_3
  TIER_4
  TIER_5
  TIER_6
  TIER_7
  TIER_8
}

enum MemberVaultSecurityTier {
  TIER_0
  TIER_1
  TIER_2
}

model MemberVault {
  userId  String
  guildId String
  member  Member @relation(fields: [userId, guildId], references: [userId, guildId], onDelete: Cascade)

  capacityTier MemberVaultCapacityTier @default(TIER_0)
  securityTier MemberVaultSecurityTier @default(TIER_0)

  guildCoins Int @default(0)

  @@id([userId, guildId])
  @@map("member_vault")
}
