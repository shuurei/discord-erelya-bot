model MemberDailyQuest {
  userId  String
  guildId String
  member  Member @relation(fields: [userId, guildId], references: [userId, guildId], onDelete: Cascade)

  voiceMinutesTarget Int?
  messagesSentTarget Int?

  voiceMinutesProgress Int @default(0)
  messagesSentProgress Int @default(0)

  startAt   DateTime?
  isClaimed Boolean   @default(false)

  @@id([userId, guildId])
  @@map("member_daily_quest")
}
